                <not><equal><clip pos="7" part="lem"/><lit v="prpers"/></equal></not>
              </and>
            </or>
            <or>
              <and>
                <equal><clip pos="4" part="lem"/><lit v="prn_nosubj"/></equal>
                <equal><clip pos="7" part="lem"/><lit v="prn_nosubj"/></equal>
              </and>
              <and>
                <not><equal><clip pos="4" part="lem"/><lit v="prn_nosubj"/></equal></not>
                <not><equal><clip pos="7" part="lem"/><lit v="prn_nosubj"/></equal></not>
