                    </or>
                  </and>
                </test>
                <call-macro n="kunordigo_nbr">
                  <with-param pos="1"/>
                  <with-param pos="4"/>
                </call-macro>
	        <out> <!-- bedaŭrinde ni ne povas krei unu solan SN pro tio, ke ni ne povas heredigi valorojn al la interna SN -->
                  <chunk>
                    <clip pos="1" part="whole"/>
                  </chunk>
	          <b pos="1"/>
	          <chunk>
	            <clip pos="2" part="whole"/>
	          </chunk>
	          <b pos="2"/>
	          <chunk>
	            <clip pos="3" part="whole"/>
	          </chunk>
	          <b pos="3"/>
                  <chunk>
                    <clip pos="4" part="whole"/>
                  </chunk>
	        </out>
	    </when>
	    <otherwise>
	      <out>
	        <chunk>
	          <clip pos="1" part="whole"/>
	        </chunk>
	        <b pos="1"/>
	        <chunk>
	          <clip pos="2" part="whole"/>
	        </chunk>
	        <b pos="2"/>
	        <chunk>
	          <clip pos="3" part="whole"/>
	        </chunk>
	        <b pos="3"/>
	        <chunk>
	          <clip pos="4" part="whole"/>
	        </chunk>
	      </out>
	    </otherwise>
          </choose>
	</otherwise>
      </choose>
    </action>
    </rule>


