  <!-- fino DEMANDOJ -->

  <!-- komenco DUOBLAJ NEOJ -->

    <rule comment="REGLA: NE SV NEK SN NEK SN  -   SV NEK SN NEK SN ">
      <pattern>
        <pattern-item n="ne"/>
        <pattern-item n="SV"/>
        <pattern-item n="nek"/>
        <pattern-item n="SN"/>
        <pattern-item n="nek"/>
        <pattern-item n="SN"/>
      </pattern>
      <action>
        <call-macro n="f_bcond"><with-param pos="1"/><with-param pos="2"/></call-macro>
        <out>
          <chunk>
            <clip pos="2" part="whole"/>
          </chunk>
          <b pos="2"/>
        </out>

<!-- Ni kunigas nur se SN1 kaj SN2 estas samnombraj; aliokaze unu el la du ŝanĝus sian nombron -->
<!-- Ni kunigas nur se SN1 kaj SN2 estas ambaŭ pronomoj aŭ malpronomoj; aliokaze la sintagmo-ĉapo malsamas kaj estiĝas bordelo -->
    <choose>
      <when>
        <test>
          <and>
            <equal><clip pos="4" part="nbr"/><clip pos="6" part="nbr"/></equal>
            <equal><clip pos="4" part="tipus_prn"/><clip pos="6" part="tipus_prn"/></equal>
            <or>
              <and>
                <equal><clip pos="4" part="lem"/><lit v="prpers"/></equal>
                <equal><clip pos="6" part="lem"/><lit v="prpers"/></equal>
              </and>
              <and>
                <not><equal><clip pos="4" part="lem"/><lit v="prpers"/></equal></not>
                <not><equal><clip pos="6" part="lem"/><lit v="prpers"/></equal></not>
              </and>
            </or>
            <or>
              <and>
                <equal><clip pos="4" part="lem"/><lit v="prn_nosubj"/></equal>
                <equal><clip pos="6" part="lem"/><lit v="prn_nosubj"/></equal>
              </and>
              <and>
                <not><equal><clip pos="4" part="lem"/><lit v="prn_nosubj"/></equal></not>
                <not><equal><clip pos="6" part="lem"/><lit v="prn_nosubj"/></equal></not>
              </and>
            </or>
          </and>
        </test>
        <out>
          <chunk>
            <get-case-from pos="4"><lit v="nom_conj_nom"/></get-case-from>
            <clip pos="4" part="tags"/>
            <lit v="{"/>
            <clip pos="3" part="content"/>
            <b pos="3"/>
            <clip pos="4" part="content"/>
            <b pos="4"/>
            <clip pos="5" part="content"/>
            <b pos="5"/>
            <clip pos="6" part="content"/>
             <lit v="}"/>
          </chunk>
        </out>
      </when>
      <otherwise>
        <out>
          <chunk>
            <clip pos="3" part="whole"/>
          </chunk>
          <b pos="3"/>
          <chunk>
            <clip pos="4" part="whole"/>
          </chunk>
          <b pos="4"/>
          <chunk>
            <clip pos="5" part="whole"/>
          </chunk>
          <b pos="5"/>
          <chunk>
            <clip pos="6" part="whole"/>
          </chunk>
        </out>
      </otherwise>
    </choose>
      </action>
    </rule>

