        <pattern-item n="SN_noitg"/>
        <pattern-item n="prn_subjenc"/>
      </pattern>
      <action>
        <out>
	  <chunk>
	    <clip pos="1" part="whole"/>
	  </chunk>
        </out>
        <call-macro n="f_bcond"><with-param pos="1"/><with-param pos="2"/></call-macro>
        <out>
	  <chunk>
	    <clip pos="2" part="whole"/>
	  </chunk>
	  <b pos="2"/>
	  <chunk>
	    <get-case-from pos="3">
	      <lit v="cnj"/>
	    </get-case-from>
	    <lit-tag v="cnjadv"/>
	    <lit v="{^"/>
	    <get-case-from pos="3">
	      <lit v="ĉu"/>
	    </get-case-from>
	    <lit-tag v="cnjadv"/>
	    <lit v="$}"/>
	  </chunk>
	  <b pos="3"/>
	</out>

        <choose>
          <when>
            <test>
              <equal>
                <case-of pos="3" part="lem"/>
