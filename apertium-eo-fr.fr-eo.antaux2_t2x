          </chunk>
          <b pos="4"/>
          <chunk>
            <clip pos="5" part="whole"/>
          </chunk>
          <b pos="5"/>
          <chunk>
            <clip pos="6" part="whole"/>
          </chunk>
          <b pos="6"/>
          <chunk>
            <clip pos="7" part="whole"/>
          </chunk>
        </out>
      </otherwise>
    </choose>
      </action>
    </rule>

    <rule comment="REGLA: NEK SN NEK SN NE SV  -   NEK SN NEK SN SV ">
      <pattern>
        <pattern-item n="nek"/>
        <pattern-item n="SN"/>
        <pattern-item n="nek"/>
        <pattern-item n="SN"/>
        <pattern-item n="ne"/>
        <pattern-item n="SV"/>
      </pattern>

      <action>
<!-- Ni kunigas nur se SN1 kaj SN2 estas samnombraj; aliokaze unu el la du ŝanĝus sian nombron -->
<!-- Ni kunigas nur se SN1 kaj SN2 estas ambaŭ pronomoj aŭ malpronomoj; aliokaze la sintagmo-ĉapo malsamas kaj estiĝas bordelo -->
    <choose>
      <when>
        <test>
