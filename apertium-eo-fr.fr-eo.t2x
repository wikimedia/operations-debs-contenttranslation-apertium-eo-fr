<rule comment="REGLA: SN SV PRO SN (EO per l'acusatiu dels SN objecte amb changecase=nom); la nena va donar-me el llibre; el clitic passa a objecte indirecte precedit per al i el SN objecte agafa cas acusatiu">
      <pattern>
	<pattern-item n="SN"/>
	<pattern-item n="SVlex"/>
	<pattern-item n="SNobj"/>
	<pattern-item n="SNacc"/>
      </pattern>
      <action>

	    <let>
	      <clip pos="4" part="case"/>
	      <lit-tag v="acc"/>
	    </let>
	    <let>
	      <clip pos="3" part="tipus_prn"/>
	      <lit-tag v="subj"/>
	    </let>

	<out>
	  <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
	    <clip pos="1" part="chcontent"/>
	  </chunk>
	  <b pos="1"/>
	  <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
	    <clip pos="2" part="chcontent"/>
	  </chunk>
	  <b pos="2"/>
	  <chunk>
	    <get-case-from pos="3">
	      <lit v="al3"/>
	    </get-case-from>
